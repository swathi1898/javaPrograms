<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Hyperlink?>

<?import javafx.scene.web.WebView?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10" stylesheets="@style.css" fx:id="gridPane">
    <Button text="Button Open.." fx:id="button1" id="button_one" onMouseClicked="#handleClick" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <Button text="Button Two" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
    <Button  text="Button Three"  GridPane.rowIndex="0" GridPane.columnIndex="2"/>
    <Button text="Button Four" GridPane.rowIndex="0" GridPane.columnIndex="3"/>
    <Button text="Button Five" GridPane.rowIndex="0" GridPane.columnIndex="4"/>

    <Label fx:id="label" text="javafx effects" GridPane.rowIndex="1" GridPane.columnIndex="0"
           onMouseEntered="#handleMouseEnter" onMouseExited="#handleMouseExit" />
    <Hyperlink text="www.javafx.com" GridPane.rowIndex="2" GridPane.columnIndex="0" onAction="#handleLinkClick"/>

    <WebView fx:id="webView" GridPane.rowIndex="5" GridPane.columnIndex="0" GridPane.columnSpan="5" GridPane.rowSpan="5"/>
</GridPane>